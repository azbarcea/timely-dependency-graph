// annotated example: https://github.com/jshint/jshint/blob/master/examples/.jshintrc
// http://jshint.com/docs/options/
{
    "esversion": 6,
    "maxerr": 50,

    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "latedef": false,
    "noarg": true,
    "nonbsp": true,
    "nonew": false,
    "plusplus": false,
    "undef": false,
    "unused": false,
    "strict": false,
    "maxparams": false,
    "maxdepth": false,
    "maxstatements": false,
    "maxcomplexity": false,
    "varstmt": false,

    "asi": true,
    "boss": false,
    "debug": false,
    "eqnull": false,
    "moz": false,
    "evil": false,
    "expr": false,
    "funcscope": false,
    "globalstrict": true,
    "iterator": false,
    "lastsemic": false,
    "loopfunc": false,
    "noyield": false,
    "notypeof": false,
    "proto": false,
    "scripturl": false,
    "shadow": true,
    "supernew": false,
    "validthis": false,

    "browser": true,
    "browserify": true,
    "couch": false,
    "devel": true,
    "dojo": false,
    "jasmine": false,
    "jquery": false,
    "mocha": false,
    "mootools": false,
    "node": false,
    "nonstandard": false,
    "phantom": false,
    "prototypejs": false,
    "qunit": false,
    "rhino": false,
    "shelljs": false,
    "typed": false,
    "worker": false,
    "wsh": false,
    "yui": false,

    "globals": {},

    "laxcomma": true
}
